    "extra": {
      "patches": {
        "drupal/core": {
          "Support third party settings for components within a section": "https://www.drupal.org/files/issues/2022-04-27/3015152-147.patch",
          "Order image mappings by breakpoint ID and numeric multiplier": "https://www.drupal.org/files/issues/2022-05-19/3267870-9.5.x-47.patch",
          "Apply width and height attributes to responsive image tag": "https://www.drupal.org/files/issues/2022-09-21/3192234-220-9.4.x.patch"
        },
        "drupal/commerce_pos": {
          "Call to deprecated function file_prepare_directory()": "https://www.drupal.org/files/issues/2022-01-24/commerce_pos-replace_deprecated_function_call-3260249-2.patch",
          "On PHP 8.1 warnings about ctype_digit": "https://www.drupal.org/files/issues/2023-08-08/php81-deprecated-ctype_digit.patch"
        }
      },
      "composer-exit-on-patch-failure": true,
      "patchLevel": {
          "drupal/core": "-p2"
      },
      "enable-patching": true
    }
