{
    "name": "drupal/recommended-project",
    "type": "project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "keywords": [
        "Drupal"
    ],
    "homepage": "https://www.drupal.org/project/drupal",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Roberto Araya",
            "email": "roberto.araya@btester.cl",
            "homepage": "https://github.com/roberto-araya/bt_store_cl",
            "role": "Developer"
        }
    ],
    "require": {
        "drupal/ctools": "~3.9.0",
        "drupal/bt_personas": "^2.x-dev",
        "drupal/bt_core": "^2.x-dev",
        "drupal/bt_cms": "^2.x-dev",
        "drupal/bt_media": "^2.x-dev",
        "drupal/bt_image": "^2.x-dev",
        "drupal/bt_cbc": "^2.x-dev",
        "drupal/bt_article": "^2.x-dev",
        "drupal/bt_basic_page": "^2.x-dev",
        "drupal/bt_blog": "^2.x-dev",
        "drupal/bt_image_gallery": "^2.x-dev",
        "drupal/bt_commerce": "^2.x-dev",
        "drupal/bt_layouts": "^1.x-dev",
        "drupal/bt_crm": "^2.x-dev",
        "drupal/commerce_pos": "^2.6",
        "drupal/commerce_stock": "^1.0",
        "drupal/components": "^3.0.0-beta2",
        "drupal/layout_builder_modal": "1.x",
        "drupal/twig_tools": "1.x",
        "drupal/cssvars": "1.x",
        "drupal/custom_markup_block": "^1.1",
        "drupal/layout_builder_restrictions": "^2.13",
        "drupal/media_library_form_element": "^2.0",
        "drupal/media_parent_entity_link": "^1.x-dev",
        "drupal/responsive_background_image": "^2.0@beta",
        "drupal/filter_perms": "^1.0@alpha",
        "drupal/views_css_grid": "^1.0.x-dev",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/menu_block": "^1.8",
        "drupal/menu_item_extras": "^2.17",
        "drupal/product_taxonomy_filter": "^3.x-dev@dev",
        "drupal/views_bootstrap": "^5.5@alpha",
        "drupal/metatag": "^1.22",
        "drupal/schema_metatag": "^2.3",
        "drupal/symfony_mailer": "^1.1@beta",
        "drupal/transbank": "^2.0@beta",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "9.4.9",
        "drupal/core-project-message": "9.4.9",
        "drupal/core-recommended": "9.4.9"
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "Support third party settings for components within a section": "https://www.drupal.org/files/issues/2022-04-27/3015152-147.patch"
            },
            "drupal/commerce_pos": {
                "Call to deprecated function file_prepare_directory()": "https://www.drupal.org/files/issues/2022-01-24/commerce_pos-replace_deprecated_function_call-3260249-2.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "phpcodesniffer-search-depth": 10
    },
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "drupal/core-dev": "^9.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
